{
  "version": 2,
  "name": "inflearn-shoppingmall-server",
  "builds": [
    { "src": "./index.js", "use": "@vercel/node" },
    {
      "src": "uploads/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" },
    {
      "src": "/(.+)",
      "dest": "uploads/$1"
    }
  ]
}
