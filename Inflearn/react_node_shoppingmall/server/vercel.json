{
  "version": 2,
  "name": "inflearn-shoppingmall-server",
  "builds": [
    { "src": "./api/index.js", "use": "@vercel/node" },
    {
      "src": "uploads/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" },
    { "src": "/uploads/(.+)", "dest": "uploads/$1" }
  ]
}
